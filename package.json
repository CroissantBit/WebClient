{
	"name": "webclient",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen": "protoc --proto_path=proto/src/ --plugin=protoc-gen-ts_proto=\".\\node_modules\\.bin\\protoc-gen-ts_proto.cmd\" --ts_proto_opt=outputTypeAnnotations=static-only --ts_proto_opt=outputTypeRegistry=true --ts_proto_opt=exportCommonSymbols=false --ts_proto_opt=esModuleInterop=true --ts_proto_opt=env=browser --ts_proto_out=./src/lib/types/ ./proto/src/*.proto"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.27.1",
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.34.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "0.4.1",
		"prettier-plugin-organize-imports": "^3.2.3",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"tailwindcss": "^3.3.5",
		"ts-proto": "^1.162.2",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	},
	"type": "module",
	"dependencies": {
		"lucide-svelte": "^0.290.0",
		"protobufjs": "^7.2.5"
	}
}
